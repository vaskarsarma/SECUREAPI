{
  "name": "secureapi",
  "version": "1.0.0",
  "description": "Sample protected API using Auth0 access token",
  "main": "server.js",
  "scripts": {
    "start:dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" src",
    "lint:fix": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "audit": "npm audit --json > audit-report.json",
    "swagger-autogen": "node src/swagger/swagger-autogen.js"
  },
  "keywords": [],
  "author": "Vaskar Sarma",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "axios-mock-adapter": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  }
}
